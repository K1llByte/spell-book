	# fi; \
	# if [ $empty -eq 1 ]; then \
	# 	echo "2nd if"; \
	# 	bspc desktop $cur --remove; \
	# fi

super + shift + {1-9,0}
    bspc node -d '^{1-9,10}' --follow

super + control + {1-9,0}
    bspc node -d '^{1-9,10}' --follow

super + control + {h,j,k,l}
    bspc node --resize {right -10 0, bottom 0 +10, bottom 0 -10, right +10 0}

super + alt + {h,l}
	bspc node --to-monitor {prev,next} --follow

super + shift + r
    bspc wm --restart ; notify-send -t 500 -u low 'reset done'

alt + Tab
	bspc desktop -f last

super + {_,shift +} Tab
	bspc desktop -f {next,prev}.local.occupied

super + e
	bspc desktop --layout next

super + {_,shift +} n
	bspc desktop --bubble {next,prev}

super + g
	bspc node -s biggest.local

super + shift + e
	bspc node --equalize

super + q ; {h,j,k,l}
	bspc node -p {west,south,north,east}

super + apostrophe
	bspc node -t \~pseudo_tiled

super + i
	bspc node $(bspc query --nodes --desktop | head -1) -g hidden
